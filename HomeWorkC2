using System;
using System.Collections.Generic;

namespace HomeWorkF3
{
    class Program
    {
        static void Main(string[] args)
        {
            Random random = new Random();
            Queue<int> countMoney = new Queue<int>();
            
            int randomNumber = random.Next(5, 11);
            int sum = 0;

            for (int i = 0; i <= randomNumber; i++)
            {
                int price = random.Next(100, 1000);
                countMoney.Enqueue(price);

                Console.WriteLine($"Стоимость покупки {countMoney.Dequeue()} рублей");

                sum += price;

                Console.WriteLine($"В кассе {sum}");

                Console.ReadKey();
                Console.Clear();
            }
        }
    }
}
